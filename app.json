
{
  "expo": {
    "name": "VIRALYZE",
    "slug": "viralyze",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/a8b69f5d-7692-41da-84fd-76aebd35c7d4.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/a8b69f5d-7692-41da-84fd-76aebd35c7d4.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F14"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.viralyze.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/a8b69f5d-7692-41da-84fd-76aebd35c7d4.png",
        "backgroundColor": "#0B0F14"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.viralyze.app"
    },
    "web": {
      "favicon": "./assets/images/a8b69f5d-7692-41da-84fd-76aebd35c7d4.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "viralyze",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
